<h1>Pages#dashboard</h1>
<p>Find me in app/views/pages/dashboard.html.erb</p>
<table class="table">
    <thead>
        <tr>
        <th scope="col">#</th>
        <th scope="col">First</th>
        <th scope="col">Last</th>
        <th scope="col">Handle</th>
        </tr>
    </thead>
    <tbody>
        <% @projects.each do |x| %>
        <tr>
            <th scope="row"><%= x.id %></th>
            <td><%= x.name %></td>
            <td><%= x.description %></td>
            <td><%= x.start_date %></td>
            <td><%= x.end_date %></td>
            <td><%= x.status %></td>
        </tr>
        <% end %>
    </tbody>
</table>
